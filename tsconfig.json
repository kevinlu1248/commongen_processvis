  "compilerOptions": {
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node"
  },
  "exclude": ["src/**/*.{ts,tsx}", "src/*.{ts,tsx}", "dist", "node_modules"]
}
